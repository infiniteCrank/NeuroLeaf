{
  "name": "neuroleaf",
  "version": "1.0.0",
  "description": "This is a javascript ELM library",
  "main": "dist/neuroleaf.umd.js",
  "module": "dist/neuroleaf.esm.js",
  "types": "dist/index.d.ts",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "rollup -c",
    "postbuild": "cp dist/neuroleaf.umd.js public/neuroleaf.umd.js",
    "watch": "rollup -c -w",
    "clean": "rm -rf dist",
    "dev": "vite",
    "dev:music": "DEMO=music-autocomplete-demo vite",
    "dev:lang": "DEMO=language-awareness-demo vite",
    "dev:chain": "DEMO=chain-models vite",
    "dev:chain2": "DEMO=chain-with-encoder vite",
    "dev:chain3": "DEMO=chain-encoder-feauture-combine vite",
    "dev:chain4": "DEMO=voting vite",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infiniteCrank/NeuroLeaf.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/infiniteCrank/NeuroLeaf/issues"
  },
  "homepage": "https://github.com/infiniteCrank/NeuroLeaf#readme",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "eslint": "^9.29.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "rollup": "^4.43.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}